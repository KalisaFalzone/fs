<div class="container">
    <div class="headline">
        <h1>A list of all Users and Videos
        </h1>
    </div>
    <div class="row">
        <div class="col s2">
        </div>
        <div class="col s4">
            <h3>Users</h3>
            <ul>
              <% @users.each do |user| %>
                <li>
                  <%=user.name%>
                </li>
              <% end %>
            </ul>
        </div>
        <div class="col s4">
            <h3>Videos</h3>
            <ul>
              <% @videos.each do |video| %>
              <li>
                <%=video.title%>
              </li>
              <% end %>
            </ul>
        </div>
        <div class="col s2">
        </div>
    </div>
</div>
